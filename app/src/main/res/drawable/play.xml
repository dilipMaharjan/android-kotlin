<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="48dp"
    android:height="48dp"
    android:viewportHeight="24"
    android:viewportWidth="24">
    <group
        android:name="scaleX"
        android:pivotX="12"
        android:pivotY="12"
        android:scaleX="1.5">
        <group
            android:name="rotation"
            android:pivotX="12"
            android:pivotY="12"
            android:rotation="180">
            <group
                android:name="translate"
                android:pivotX="12"
                android:pivotY="12"
                android:translateX="1">
                <path
                    android:name="play"
                    android:fillColor="#FF9800"
                    android:pathData="M 8 5 L 8 19 L 19 12 Z"
                    />
                <!-- This path draws two green stroked vertical pause bars. -->
            </group>
        </group>
    </group>
</vector>